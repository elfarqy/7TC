<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sqlite.modul.yufri.databasesqlite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MenuActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TambahActivity"
            android:label="@string/activity_tambah_data" >
        </activity>
        <activity
            android:name=".DataActivity"
            android:label="@string/activity_data_barang" >
        </activity>
        <activity
            android:name=".UbahActivity"
            android:label="@string/activity_ubah_data" >
        </activity>
    </application>

</manifest>
